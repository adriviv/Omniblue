{
  "manifest_version": 3,
  "name": "Omniblue_production",
  "description": "manifest.json description",
  "version": "0.0.0",
  "icons": {
    "16": "icon/16.png",
    "32": "icon/32.png",
    "48": "icon/48.png",
    "96": "icon/96.png",
    "128": "icon/128.png"
  },
  "host_permissions": [
    "http://localhost:3000/*",
    "http://localhost:3001/*",
    "http://127.0.0.1:3000/*",
    "http://127.0.0.1:3001/*",
    "https://salesforce-back-api-staging.herokuapp.com/*",
    "https://salesforce-back-api-f704ca957913.herokuapp.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "omniblue_icon.png",
        "omniblue_icon_active.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Default Popup Title",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-scripts/content.js"
      ]
    }
  ]
}